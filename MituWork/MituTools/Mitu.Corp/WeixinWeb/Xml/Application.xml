<?xml version="1.0" encoding="utf-8"?>
<tk:Toolkit version="5.0" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.qdocuments.net schema\v5\WebApp.xsd">
  <tk:Application CommandTimeout="600" Path=".." Culture="zh-CHS" UseCache="false" CacheTime="00:01:00" AppPath="WebSite/Bin">
    <tk:Url StartupPath="~/Library/WebListXmlPage.tkx?Source=Cxcs/FaGui" ErrorPage="~/error/error.htm"/>
  </tk:Application>
  <tk:Debug Debug="true" ShowException="true"/>
  <tk:Databases>
    <tk:Database Default="true" SqlProvider="SQL Server2005" Name="Tennis" ConnectionString="USER ID=mitu;PASSWORD=hello;INITIAL CATALOG=mitu.hello;DATA SOURCE=192.168.1.8;CONNECT TIMEOUT=30" DbProvider="SQL Server" ProviderName="System.Data.SqlClient"/>
    <tk:Database Default="false" SqlProvider="SQL Server2005" Name="Weixin" ConnectionString="USER ID=mitu;PASSWORD=hello;INITIAL CATALOG=mitu.hello;DATA SOURCE=192.168.1.8;CONNECT TIMEOUT=30" DbProvider="SQL Server" ProviderName="System.Data.SqlClient"/>
  </tk:Databases>
  <tk:SecretKey Key="hello" IV="AAAAAAAAAAAAAAAAAAAAAA=="/>
  <tk:Hosts>
    <tk:Host Key="cxcs" Value="http://cxcs.mituyun.com"/>
  </tk:Hosts>
  <tk:ExceptionHandler>
    <tk:ErrorOperationException>
      <tk:PageMakerExceptionHandler>
        <tk:PageMaker>
          <tk:FreeRazorPageMaker RazorFile="razortemplate\BootCss\Bin\Exception.cshtml"/>
          <!--<tk:FreeRazorPageMaker RazorFile="Razor\Error\Error.cshtml"/>-->
        </tk:PageMaker>
      </tk:PageMakerExceptionHandler>
    </tk:ErrorOperationException>
    <tk:ToolkitException>
      <tk:PageMakerExceptionHandler>
        <tk:PageMaker>
          <tk:FreeRazorPageMaker RazorFile="razortemplate\BootCss\Bin\Exception.cshtml"/>
        </tk:PageMaker>
      </tk:PageMakerExceptionHandler>
    </tk:ToolkitException>
    <tk:Exception>
      <tk:PageMakerExceptionHandler Log="true">
        <tk:PageMaker>
          <tk:FreeRazorPageMaker RazorFile="razortemplate\BootCss\Bin\Exception.cshtml"/>
        </tk:PageMaker>
      </tk:PageMakerExceptionHandler>
    </tk:Exception>
  </tk:ExceptionHandler>
  <tk:DefaultConfig>
    <tk:DefaultPostObjectCreator>
      <tk:CompositeCreator>
        <tk:Item>
          <tk:Condition SearchType="Style" Style="List"/>
          <tk:JsonObjectCreator RegClassName="QueryConditionObject"/>
        </tk:Item>
        <tk:Item>
          <tk:Condition SearchType="True"/>
          <tk:JsonPostDataSetCreator/>
        </tk:Item>
      </tk:CompositeCreator>
    </tk:DefaultPostObjectCreator>
    <tk:DefaultPageMaker>
      <tk:XmlPageMaker/>
    </tk:DefaultPageMaker>
    <tk:DefaultRedirector>
      <tk:OutputRedirector/>
    </tk:DefaultRedirector>
    <!--<tk:ExceptionPageMaker>
      <tk:XmlPageMaker/>
    </tk:ExceptionPageMaker>-->
  </tk:DefaultConfig>
  <tk:Upload TempPath="UploadTemp" TempVirtualPath="~/UploadTemp/" Path="Upload" VirtualPath="~/Upload/"/>
</tk:Toolkit>
