@using System;
@using System.IO;
@using System.Collections.Generic;
@{
    string path = (string)(Model["SolutionPath"]);
    path = Path.Combine(path, "Xml");
    int length = path.Length;
    if (!path.EndsWith(@"\"))
        ++length;
    var files = Directory.EnumerateFiles(path, "*.cshtml", SearchOption.AllDirectories);
}
