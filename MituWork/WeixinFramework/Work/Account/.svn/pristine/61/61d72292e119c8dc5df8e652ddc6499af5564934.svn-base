<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit version="5.0" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\Module.xsd">
  <tk:Module>
    <tk:Title>
      <tk:Content>报表数据</tk:Content>
    </tk:Title>
    <tk:MetaData>
      <tk:SingleXmlMetaData DataXml="Accounting/ReportData.xml"/>
    </tk:MetaData>
    <tk:Source>
      <tk:SingleDbSource OrderBy="ORDER BY RD_REPORT_DATE DESC, RD_REPORT_NAME">
        <tk:Resolver>
          <tk:RegResolver>ReportData</tk:RegResolver>
        </tk:Resolver>
        <tk:FilterSql NeedParse="true">RD_COMPANY = {#Company}</tk:FilterSql>
        <tk:Operators>
          <tk:StandardOperators>
            <tk:Right>
              <tk:FieldOperateRight FieldName="ReportName">
                <tk:Empty>InsertProfit</tk:Empty>
                <tk:Item Value="ProfitAppropriation">UpdateProfit</tk:Item>
              </tk:FieldOperateRight>
            </tk:Right>
            <tk:Operator IconClass="icon-plus" Position="Global" Id="InsertProfit">
              <tk:Caption>
                <tk:Content>新建利润分配表</tk:Content>
              </tk:Caption>
              <tk:Content NeedParse="true">~/Library/WebInsertXmlPage.tkx?Source=Report/ProfitAppropriation&amp;Company={#Company}</tk:Content>
            </tk:Operator>
            <tk:Operator Id="UpdateProfit">
              <tk:Caption>
                <tk:Content>修改</tk:Content>
              </tk:Caption>
              <tk:Content>~/Library/WebInsertXmlPage.tkx?Source=Report/ProfitAppropriation&amp;ReportId=*ReportId*</tk:Content>
            </tk:Operator>
          </tk:StandardOperators>
        </tk:Operators>
      </tk:SingleDbSource>
    </tk:Source>
    <tk:PageMaker>
      <tk:SingleRazorPageMaker ListDisplay="Striped">
        <tk:Item Style="List">
          <tk:RazorData>
            <tk:NormalListData OperatorWidth="100" SearchCheckBox="Like"/>
          </tk:RazorData>
        </tk:Item>
      </tk:SingleRazorPageMaker>
    </tk:PageMaker>
  </tk:Module>
</tk:Toolkit>
