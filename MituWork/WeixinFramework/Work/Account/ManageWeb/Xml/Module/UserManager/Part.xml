    <tk:Source>
      <tk:SingleDbSource>
        <tk:Resolver>
          <tk:RegResolver>Part</tk:RegResolver>
        </tk:Resolver>
        <tk:Operators>
          <tk:StandardOperators>
            <tk:Right>
              <tk:EmptyOperateRight/>
