<?xml version="1.0" encoding="UTF-8"?>
<tk:Toolkit version="5.0" xsi:schemaLocation="http://www.qdocuments.net ..\..\schema\v5\DataXml.xsd" xmlns:tk="http://www.qdocuments.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tk:Table TableName="CS_BINDING">
    <tk:TableDesc>
      <tk:Content>用户绑定</tk:Content>
    </tk:TableDesc>
    <tk:Field DataType="int" IsKey="true" IsEmpty="false">
      <tk:FieldName>CB_BIND_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>ID</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Hidden" Order="10" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>CB_TITLE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>姓名职务</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length> 
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Text" Order="20" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>CB_PHONE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>手机号码</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length> 
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Text" Order="30" DefaultShow="All"/>
      <tk:Constraints>
        <tk:Mobile />
      </tk:Constraints>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>CB_REGISTER_NUM</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>税务登记号</tk:Content>
      </tk:DisplayName>
      <tk:Length>200</tk:Length> 
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Text" Order="40" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>CB_TAX_NAME</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>纳税户名称</tk:Content>
      </tk:DisplayName>
      <tk:Length>200</tk:Length> 
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Text" Order="50" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string" IsEmpty="false">
      <tk:FieldName>CB_TAX_NUMBER</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>纳税编码</tk:Content>
      </tk:DisplayName>
      <tk:Length>200</tk:Length> 
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Text" Order="60" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>CB_OPEN_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>OpenId</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length> 
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Hidden" Order="70" DefaultShow="All"/>
    </tk:Field>
    <tk:Field DataType="short">
      <tk:FieldName>CB_CHECK_FLAG</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>审核标志</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="CheckBox" Order="80" DefaultShow="None"/>
      <tk:Extension CheckValue="1" UnCheckValue="0"/>
    </tk:Field>
    <tk:Field DataType="int">
      <tk:FieldName>CB_CHECK_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>审核人</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Text" Order="90" DefaultShow="None"/>
    </tk:Field>
    <tk:Field DataType="Date">
      <tk:FieldName>CB_CHECK_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>审核时间</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Text" Order="100" DefaultShow="None"/>
    </tk:Field>
    <tk:Field DataType="string">
      <tk:FieldName>CB_CREATE_ID</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>创建人</tk:Content>
      </tk:DisplayName>
      <tk:Length>50</tk:Length> 
      <tk:EasySearch RegName="USER"/>
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="EasySearch" Order="110" DefaultShow="None"/>
    </tk:Field>
    <tk:Field DataType="DateTime">
      <tk:FieldName>CB_CREATE_DATE</tk:FieldName>
      <tk:DisplayName>
        <tk:Content>创建时间</tk:Content>
      </tk:DisplayName>
      <tk:Layout Layout="PerUnit" UnitNum="1" />
      <tk:Control Control="Text" Order="120" DefaultShow="None"/>
    </tk:Field>
  </tk:Table>
</tk:Toolkit>